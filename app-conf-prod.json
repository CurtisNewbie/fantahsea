{
  "mode": {
    "production": true
  },
  "mysql": {
    "enabled": true,
    "user": "${mysqlusername}",
    "password": "${mysqlpassword}",
    "database": "fantahsea",
    "host": "${mysqladdr}",
    "port": "3306"
  },
  "redis": {
    "enabled": true,
    "address": "redis",
    "port": "6379",
    "username": "",
    "password": "",
    "database": 0
  },
  "server": {
    "host": "0.0.0.0",
    "port": "8081"
  },
  "file": {
    "base": "/usr/src/fantahse/files"
  },
  "client": {
    "fileServiceUrl": "http://file-service:8082"
  },
  "consul": {
    "enabled": true,
    "registerName": "fantahsea",
    "consulAddress": "consul:8500",
    "healthCheckUrl": "/health",
    "healthCheckInterval": "60s",
    "healthCheckTimeout": "5s",
    "healthCheckFailedDeregisterAfter": "5s"
  }
}